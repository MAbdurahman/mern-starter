{
  "name": "mern-starter",
  "version": "1.0.0",
  "description": "MERN Stack boilerplate starter",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "SET NODE_ENV=DEVELOPMENT& nodemon server/server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "prod": "SET NODE_ENV=PRODUCTION& nodemon server/server"
  },
  "repository": "https://github.com/MAbdurahman/mern-starter.git",
  "author": "Mahdi Abdurrahman <mdbdrrhm@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-validator": "^6.12.0",
    "google-auth-library": "^7.1.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.13",
    "morgan": "^1.10.0",
    "nodemailer": "^6.6.1"
  },
  "devDependencies": {
    "concurrently": "^6.2.0"
  }
}
